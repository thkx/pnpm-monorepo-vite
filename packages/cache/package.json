{
  "name": "@pnpm-monorepo-vite/cache",
  "version": "0.1.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test:i",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "npm run test && npm run build",
    "commit": "git-cz",
    "setup:semantic-release": "npx semantic-release-cli setup"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hhhhhhhxx/packages/cache"
  },
  "license": "MIT",
  "devDependencies": {
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ]
  },
  "files": [
    "dist/"
  ],
  "description": "cache"
}